#include <stdio.h>

int main() {
    int n, i;
    int a = 0, b = 1, c;

    printf("Enter number of terms: ");
    scanf("%d", &n);

    if (n <= 0) {
        printf("Please enter a positive number");
    } else if (n == 1) {
        printf("Fibonacci sequence:\n%d", a);
    } else {
        printf("Fibonacci sequence:\n%d %d ", a, b);
        for (i = 2; i < n; i++) {
            c = a + b;
            printf("%d ", c);
            a = b;
            b = c;
        }
    }

    return 0;
}
